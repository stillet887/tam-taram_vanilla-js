<div class="footer container-fluid">
    <ul class="footer__group footer__nav-links">
        <% const nav_links = ['privacy policy', 'terms of use', 'contact us', 'newsletter', 'sitemap']; %>
        <% nav_links.forEach( link => { %>
            <li class="footer__item footer__item_wide"><a href="#" class="footer__link"><%= link %></a></li>
        <% }) %>
    </ul>
    <ul class="footer__group footer__social-networks">
        <% const social_networks = [
            { icon: 'img/ico_facebook.svg', link: 'https://www.facebook.com' },
            { icon: 'img/ico_twitter.svg', link: 'https://twitter.com' },
            { icon: 'img/ico_pinterest.svg', link: 'https://www.pinterest.com' },
            { icon: 'img/ico_youtube.svg', link: 'https://www.youtube.com' },
            { icon: 'img/ico_instagram.svg', link: 'https://www.instagram.com' }
        ] %>
        <% social_networks.forEach( network => { %>
        <li class="footer__item"><a href= <%= network.link %> class="footer__link"><img class="footer__icon" src= <%= network.icon %>></a></li>
        <% }) %>
    </ul>
    <ul class="footer__group footer__params">
        <% const params = [
            { name: 'location', value: 'United States' },
            { name: 'language', value: 'english' }
        ] %>
        <% params.forEach( param => { %>
        <li class="footer__param">
            <span class="footer__param-name"><%= param.name %>: </span>
            <span class="footer__param-value"><%= param.value %></span>
        </li>
        <% }) %>
    </ul>
</div>


